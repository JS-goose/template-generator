{
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  },
  "functions": {
    "api/gpt-email.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/rss",
      "dest": "/api/rss.js",
      "headers": {
        "Cache-Control": "s-maxage=3600, stale-while-revalidate"
      }
    },
    {
      "src": "/api/enrich-rss-data",
      "dest": "/api/enrich-rss-data.js"
    }
  ]
}
