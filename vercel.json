{
  "functions": {
    "api/rss.js": {
      "runtime": "nodejs",
      "memory": 128,
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/rss",
      "dest": "/api/rss.js",
      "headers": {
        "Cache-Control": "s-maxage=3600, stale-while-revalidate"
      }
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  }
}
